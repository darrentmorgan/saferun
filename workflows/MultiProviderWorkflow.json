        "jsonParameters": true,
        "headerParametersJson": "{\"Authorization\":\"Bearer {{$env.ANTHROPIC_API_KEY}}\",\"Content-Type\":\"application/json\",\"X-RunSafe-Purpose\":\"concierge\"}",
        "bodyParametersJson": "{\"model\":\"claude-3-sonnet-20240229\",\"max_tokens\":1024,\"messages\":[{\"role\":\"user\",\"content\":\"You are a hotel concierge AI. Do not request or include guest passport numbers, credit card details, or sensitive health info. If provided, replace with placeholders. Guest query: {{$json.query}}\"}]}"
