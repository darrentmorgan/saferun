        "bodyParametersJson": "{\"model\":\"text-embedding-ada-002\",\"input\":\"{{$json.query}}\"}"
      },
      "id": "5",
      "name": "Generate Embeddings",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [850, 300],
      "credentials": {}
    },
    {
      "parameters": {
        "options": {},
